package com.zikrullah.App;

/**
 * Hello world!
 *
 */
public class App 
{
    public static void main( String[] args )
    {
        System.out.println( "Hello World!" );
        for(int i  = 0;i<10;i++) {
        	Pair p = new Pair(i,i+1);
        	p.printPair();
        }
    }
}

class Pair{
	int x,y;
	public Pair(int x, int y) {
		this.x = x;
		this.y = y;
	}
	public void printPair() {
		System.out.println(this.x + " "+ this.y);
	}
}
